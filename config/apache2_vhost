<VirtualHost *:80>
    ServerName dev
    DocumentRoot /var/www

    <Directory /var/www>
        Options +Indexes +FollowSymLinks -MultiViews
        AllowOverride All
        AddHandler mod_python .py
        PythonHandler mod_python.publisher
        PythonDebug On
    </Directory>

    ErrorLog /vagrant/logs/apache.log

    Include "/etc/phpmyadmin/apache.conf"
</VirtualHost>
